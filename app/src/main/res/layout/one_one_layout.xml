<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_green_gradient"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/back"
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_back_arrow_white"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"/>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"

        android:background="@drawable/dark_green_no_shadow"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:src="@drawable/asst_booked"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/guideline11"
        app:layout_constraintBottom_toTopOf="@id/guideline56"
        app:layout_constraintWidth_percent="0.58"/>

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/mons_black"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="-28.2sp"
        app:layout_constraintBottom_toTopOf="@id/guideline11"
        android:text="Interactive Call"
        android:textColor="#ffffff"
        android:textSize="38.3sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/orange"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/orangle_rounded_rect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView"
        app:layout_constraintWidth_percent="0.86" />

    <TextView
        android:id="@+id/text"
        android:fontFamily="@font/mons_bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="-12sp"
        android:text="Your mentor will call you for the live\n
Interactice Session\nOn"
        app:layout_constraintBottom_toTopOf="@id/guideline75"
        android:textColor="#ffffff"
        android:textSize="16.5sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/orange" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.11" />
  <TextView  android:id="@+id/empty_link"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
      android:fontFamily="@font/mons_bold"
      android:paddingTop="5dp"
      android:paddingBottom="5dp"
      android:gravity="center"
      android:visibility="invisible"
android:text="Link will be provided soon"
      android:textColor="@color/white"
    android:background="@drawable/orangle_rounded_rect"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintWidth_percent="0.66"
    app:layout_constraintTop_toBottomOf="@id/guideline75"/>
    <LinearLayout
        android:id="@+id/linear_link"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:background="@drawable/orangle_rounded_rect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintWidth_percent="0.66"
        app:layout_constraintTop_toBottomOf="@id/guideline75">

        <TextView
            android:id="@+id/copy"
            android:paddingBottom="5dp"

            android:paddingTop="5dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15.2sp"
            android:textColor="#ffffff"
            android:lineSpacingExtra="-11.2sp"
            android:gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@drawable/selector_orange_button"
            android:clickable="true"
            android:text="Copy Link"
            android:fontFamily="@font/mons_bold"/>
        <TextView

            android:background="@drawable/white_half_curved_no_shadow"
            android:textColorLink="#f68b0f"
            android:id="@+id/link"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15.2sp"
            android:textColor="#f68b0f"
            android:lineSpacingExtra="-11.2sp"
            android:gravity="center"
            android:layout_weight="1"

            android:fontFamily="@font/mons_bold"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:background="@drawable/orangle_rounded_rect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintWidth_percent="0.66"
        app:layout_constraintBottom_toTopOf="@+id/change"
        app:layout_constraintTop_toBottomOf="@id/empty_link">

        <TextView
            android:id="@+id/date"

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15.2sp"
            android:textColor="#ffffff"
            android:lineSpacingExtra="-11.2sp"
            android:gravity="center_horizontal"
            android:layout_weight="1"
            android:text="dd/mm"
            android:fontFamily="@font/mons_bold"/>
        <TextView
            android:id="@+id/time"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15.2sp"
            android:textColor="#ffffff"
            android:lineSpacingExtra="-11.2sp"
            android:gravity="center_horizontal"
            android:layout_weight="1"

            android:text="4:00 pm"
            android:fontFamily="@font/mons_bold"/>
    </LinearLayout>
    <TextView
        android:id="@+id/change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Change Slot"
        android:fontFamily="@font/mons_bold"
        android:textColor="#ffffff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/selector_orange_button"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/linear"
        app:layout_constraintBottom_toBottomOf="parent"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75239396" />
</androidx.constraintlayout.widget.ConstraintLayout>
