<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.karrit.xoog">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/xoog"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/xoog_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".pose_day"></activity>
        <activity android:name=".Task_finish" />
        <activity android:name=".School_program" />
        <activity android:name=".TermsAndConditions" />
        <activity
            android:name=".exer_do_basic"
            android:screenOrientation="landscape" />
        <activity
            android:name=".exer_learn"
            android:screenOrientation="landscape" />
        <activity android:name=".activity_log" />
        <activity android:name=".Parent" />
        <activity android:name=".Order_confirmed" />
        <activity android:name=".Add_Address" />
        <activity android:name=".Shop_single_item" />
        <activity
            android:name=".exercise_do1"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".excercise_do"
            android:screenOrientation="landscape" />
        <activity android:name=".task_activity_rubik" />
        <activity android:name=".Task_list" />
        <activity android:name=".slot_book" />
        <activity
            android:name=".animation_activity"
            android:screenOrientation="landscape" />
        <activity android:name=".setting_up" />
        <activity android:name=".choose_subscription" />
        <activity android:name=".Home_page" />
        <activity android:name=".account_subscipe" />
        <activity android:name=".Parent_kid" />
        <activity android:name=".choose_plan" />
        <activity android:name=".referra" />
        <activity android:name=".kids_account" />
        <activity android:name=".verification_done" />
        <activity android:name=".Verification" />
        <activity android:name=".login" />
        <activity android:name=".GetStarted" />
        <activity android:name=".menu_book_call" />

        <receiver android:name=".CallReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
            </intent-filter>
        </receiver>

        <service
            android:name=".VideoService"
            android:enabled="true"
            android:exported="true" />
        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
          <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="xoog.info"
                    android:scheme="https" />
                <data
                    android:host="xoog.info"
                    android:scheme="http" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>